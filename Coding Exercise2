package myArray;

import java.util.Scanner;

public class Exercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//The variable list1 and list2 are reference arrays that each have 5 elements. Write code that copies the values in list1 to list2. Values in list1 are input by user.
		Scanner sc = new Scanner(System.in);
		
		int list1[] = new int[5];
		int list2[] = new int[5];
		System.out.println("Enter 5 elements of array : ");
		for(int x=0; x<list1.length; x++)
			list1[x] = sc.nextInt();
		
		for(int y=0; y<list2.length; y++)
			list2[y] = list1[y];
		
		for(int z=0; z<list2.length; z++)
		System.out.println(list1[z] + "" + list2[z]);
		
	}

}
